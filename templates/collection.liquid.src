<div id="app"></div>
<script>
    var products = {
        categoryId: {{ collection.id }},
        title:{{ collection.title | json }},
    {% if collection.metafields.vaithuhay.slides %}
        slides:{{ collection.metafields.vaithuhay.slides }},
    {% else %}
        slides: {},
    {% endif %}
        list: [{% for product in collection.products %}{% include 'product-json.src' with product %}{% unless forloop.last %},{% endunless %}{% endfor %}]
    };
</script>
